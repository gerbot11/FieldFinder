<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" xmlns:local="clr-namespace:FieldFinder"
            x:Class="FieldFinder.BottomMenu"
            xmlns:android="clr-namespace:Xamarin.Forms.PlatformConfiguration.AndroidSpecific;assembly=Xamarin.Forms.Core" xmlns:local1="clr-namespace:FieldFinder.Page.Main" x:DataType="local:BottomMenu"
            android:TabbedPage.ToolbarPlacement="Bottom"
            android:TabbedPage.BarItemColor="Black"
            android:TabbedPage.BarSelectedItemColor="Red"
            android:TabbedPage.IsSmoothScrollEnabled="True"
            NavigationPage.HasNavigationBar="False">
  <!--Pages can be added as references or inline-->
    
    <NavigationPage Title="Profile" IconImageSource="user.png">
        <x:Arguments>
            <local1:Profile></local1:Profile>
        </x:Arguments>
    </NavigationPage>
</TabbedPage>